{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es6"],
    "module": "ESNext",
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "sourceMap": true,
  }
}